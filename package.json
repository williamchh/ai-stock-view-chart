{
  "name": "ai-stock-view-chart",
  "version": "1.0.10",
  "description": "Interactive stock charting library with candlestick, line charts, crosshair sync, overlays, and theming.",
  "main": "dist/stock-chart.min.js",
  "module": "dist/stock-chart.js",
  "types": "dist/stock-chart.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "serve .",
    "build": "npm run build:npm && npm run build:umd",
    "build:npm": "rollup -c --environment BUILD:npm",
    "build:umd": "rollup -c --environment BUILD:umd"
  },
  "keywords": [
    "stock",
    "finance",
    "chart",
    "candlestick",
    "technical-analysis"
  ],
  "author": "Will",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup": "^4.46.2",
    "rollup-plugin-copy": "^3.5.0",
    "serve": "^14.0.0",
    "terser": "^5.43.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/williamchh/ai-stock-view-chart.git"
  },
  "bugs": {
    "url": "https://github.com/williamchh/ai-stock-view-chart/issues"
  },
  "homepage": "https://github.com/williamchh/ai-stock-view-chart#readme"
}
